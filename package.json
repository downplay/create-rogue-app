  "homepage": "./",
  "dependencies": {
    "@reduxjs/toolkit": "^1.2.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/nearley": "^2.11.1",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/redux": "^3.6.0",
    "@types/styled-components": "^5.0.1",
    "@types/uuid": "^7.0.0",
    "immer": "^8.0.1",
    "keycode-js": "^2.0.3",
    "modern-css-reset": "^1.1.0",
    "moo": "^0.5.1",
    "nearley": "^2.19.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts": "3.4.0",
    "redux": "^4.0.5",
    "styled-components": "^5.0.1",
    "typescript": "~3.7.2",
    "uuid": "^7.0.1"
  },
  "scripts": {
    "start": "cross-env EXTEND_ESLINT=true react-scripts start",
    "build": "cross-env EXTEND_ESLINT=true react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "grammar": "nearleyc src/engine/text/herotext.ne -o src/engine/text/herotext.js",
    "grammar-test-built": "nearleyc src/engine/text/herotext.js",
    "grammar-test": "npm-run-all grammar grammar-test-built"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "react-hooks/rules-of-hooks": "off",
      "react-hooks/exhaustive-deps": "off",
      "jsx-a11y/accessible-emoji": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.0",
    "npm-run-all": "^4.1.5"
  }
}
